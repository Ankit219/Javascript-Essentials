// question 2. program to convert minutes to seconds.


let minutes = 30;

seconds = minutes * 60;
console.log("30" + "Min" + "into sec is" + seconds);




// question 3. program to search element in a array of strings.

var array = [-10, -20, 30, -40, -50];

var found = array.find(function (element) {return element > 0;
})
console.log(found);


// question 4. program to display only element containing 'a' in the term of array 

array = ["Apps", "Apple", "Animals", "Fruits", "Banana", "Cat", "Dog",] ;
console.log(array);


// question 5. Print an array in reverse order

Array = ["Animal","Fruits","Vegetables"];

console.log(Array);

Array = ["Animal","Fruits","Vegetables"], reverse();
console.log(Array);

